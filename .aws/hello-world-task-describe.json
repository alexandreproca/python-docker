{
    "taskDefinition": {
        "taskDefinitionArn": "arn:aws:ecs:eu-central-1:981847621073:task-definition/Hello-World-task:1",
        "containerDefinitions": [
            {
                "name": "Hello-World",
                "image": "sample-app",
                "cpu": 0,
                "portMappings": [
                    {
                        "containerPort": 80,
                        "hostPort": 80,
                        "protocol": "tcp",
                        "name": "hello-world-80-tcp",
                        "appProtocol": "http"
                    }
                ],
                "essential": true,
            }
        ]
    }
}